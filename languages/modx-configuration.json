{
  "comments": {
    "blockComment": [ "[[-", "]]" ]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    { "open": "'", "close": "'", "notIn": ["string", "comment"] },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "`", "close": "`", "notIn": ["string", "comment"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\[\\[\\b",
      "end": "^\\s*\\]\\]\\b"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "\\[\\[[^\\]\\]]*$",
    "decreaseIndentPattern": "[^\\[\\[]*\\]\\]"
  }
}
